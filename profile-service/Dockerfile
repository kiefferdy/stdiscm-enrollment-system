FROM eclipse-temurin:17-jre

WORKDIR /app

# Use the actual JAR name generated by Maven
COPY target/profile-service-1.0-SNAPSHOT.jar app.jar

# Correct port is likely 8085 based on docker ps output, but Dockerfile says 8084. 
# Keeping 8084 as per Dockerfile content for now. Verify if needed.
EXPOSE 8084 

ENTRYPOINT ["java", "-jar", "app.jar"]
